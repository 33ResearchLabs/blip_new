{
  "name": "settlement-core",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./db": {
      "types": "./dist/db/client.d.ts",
      "import": "./dist/db/client.js",
      "require": "./dist/db/client.js"
    },
    "./finalization": {
      "types": "./dist/finalization/index.d.ts",
      "import": "./dist/finalization/index.js",
      "require": "./dist/finalization/index.js"
    },
    "./state-machine": {
      "types": "./dist/state-machine/index.d.ts",
      "import": "./dist/state-machine/index.js",
      "require": "./dist/state-machine/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3"
  }
}
